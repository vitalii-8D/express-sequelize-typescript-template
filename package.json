{
  "name": "expresstypescript",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/index.ts\"",
    "env:up": "docker compose up -d",
    "env:down": "docker compose down",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "express-validator": "^6.14.3",
    "http-status-codes": "^2.2.0",
    "mysql2": "^3.1.0",
    "pino-pretty": "^9.1.1",
    "redis": "^4.6.4",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
